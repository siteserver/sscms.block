@page
@{ Layout = "_Layout"; }

<el-form size="small" ref="form" :model="form" label-width="120px">
  <el-form-item v-if="channels" label="拦截以下页面" prop="channelIds" :rules="{ required: true, message: '请选择需要拦截的页面' }">
    <el-cascader
      v-model="form.channelIds"
      :options="channels"
      :props="{ checkStrictly: true, multiple: true }"
      filterable
      placeholder="请选择页面">
    </el-cascader>
  </el-form-item>
</el-form>

<el-divider></el-divider>
<el-row align="right" style="margin-right: 5px; margin-top: 10px;">
  <el-col :span="24" align="right">
    <el-button size="small" type="primary" v-on:click="btnSubmitClick">确 定</el-button>
    <el-button size="small" v-on:click="btnCancelClick">取 消</el-button>
  </el-col>
</el-row>

@section Scripts{
  <script src="/assets/block/js/settingsLayerChannelAdd.js" type="text/javascript"></script>
}